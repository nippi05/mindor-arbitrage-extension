{
  "description": "Scrapes data from unibet and bet365 and puts into csv file for importing to excel",
  "manifest_version": 2,
  "name": "Mindor Arbitrage Extension",
  "version": "0.1",
  "homepage_url": "https://github.com/sackidude/mindor-arbitrage-extension",
  "icons": {
    "48": "icons/logo.png"
  },

  "permissions": ["activeTab"],

  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "Mindor Arbitrage Scraper",
    "default_popup": "popup/choose_beast.html"
  }
}
